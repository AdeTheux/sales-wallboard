<p id="notice"><%= notice %></p>

<h1>Iframes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th>Department</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @iframes.each do |iframe| %>
      <tr>
        <td><%= iframe.title %></td>
        <td><%= iframe.url %></td>
        <td><%= iframe.department %></td>
        <td><%= link_to 'Show', iframe %></td>
        <td><%= link_to 'Edit', edit_iframe_path(iframe) %></td>
        <td><%= link_to 'Destroy', iframe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Iframe', new_iframe_path %>
